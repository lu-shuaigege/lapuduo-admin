        }

        .site-doc-icon li {
            width: 222px;
            border: none
        }

        .site_2 li {
            /*margin: 0 15px;*/
            width: 50%;
        }

        .site_1 li {
            margin: 0 auto;
        }

        .site-doc-icon li .layui-anim {
            width: 180px;
            height: 180px;
            line-height: 180px;
            margin: 0 auto 10px;
            text-align: center;
            background-color: #009688;
            cursor: pointer;
            color: #fff;
            border-radius: 50%;
        }

        .site-doc-icon li .layui-anim {
            font-size: 28px;
            font-weight: bold;
        }

        .site_1 li .layui-anim {
            border-radius: 6%;
            height: 100px;
            line-height: 100px;
        }
